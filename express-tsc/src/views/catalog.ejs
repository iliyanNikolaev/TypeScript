<h1>catalog page</h1>

<% if(!cars) {%>
    <p>error in db</p>
    <% } %>

        <% if(cars?.length==0) { %>
            <p>no records in db</p>
            <% } %>
                <ul>
                    <% cars?.map(function(car){ %>
                        <li>
                            brand: <%= car.brand %>,
                                model: <%= car.model %>,
                                year: <%= car.year %>

                        </li>
                        <% }) %>
                </ul>